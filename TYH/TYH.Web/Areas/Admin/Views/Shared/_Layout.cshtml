<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Gentelella Alela! | </title>
    <!-- Bootstrap -->
    <link href="~/wwwroot/lib/gentelella/vendors/bootstrap/dist/css/bootstrap.css" rel="stylesheet"/>

    <!-- Font Awesome -->
    <link href="~/wwwroot/lib/font-awesome/css/font-awesome.css" rel="stylesheet"/>

    <!-- NProgress -->
    <link href="~/wwwroot/lib/nprogress/nprogress.css" rel="stylesheet"/>

    <!-- bootstrap-daterangepicker -->
    <link href="~/wwwroot/lib/gentelella/vendors/bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.css" rel="stylesheet"/>

    <!-- Custom Theme Style -->
    <link href="~/wwwroot/lib/gentelella/build/css/custom.css" rel="stylesheet"/>

</head>
<body class="nav-md">
<div class="container body">
    <div class="main_container">
        <div class="col-md-3 left_col">
            <div class="left_col scroll-view">
                @await Html.PartialAsync("_Menu")
            </div>
        </div>
        <!-- top navigation -->
        <div class="top_nav">
            <div class="nav_menu">
                @await Html.PartialAsync("_Navigation")
            </div>
        </div>
        <!-- /top navigation -->
        <!-- page content -->
        <div class="right_col" role="main">
            <div class="">
                @RenderBody()
            </div>
        </div>
        <!-- /page content -->
        <!-- footer content -->
        <footer>
            <div class="pull-right">
                Admin Template
            </div>
            <div class="clearfix"></div>
        </footer>
        <!-- /footer content -->
    </div>
</div>
<!-- jQuery -->
<script src="~/wwwroot/lib/jquery/dist/jquery.js"></script>

<!-- Bootstrap -->
<script src="~/wwwroot/lib/bootstrap/dist/js/bootstrap.js"></script>

<!-- FastClick -->
<script src="~/wwwroot/lib/fastclick/lib/fastclick.js"></script>

<!-- NProgress -->
<script src="~/wwwroot/lib/nprogress/nprogress.js"></script>

<!-- Chart.js -->
<script src="~/wwwroot/lib/gentelella/vendors/Chart.js/dist/Chart.js"></script>
<script src="~/wwwroot/lib/gentelella/vendors/jquery-sparkline/dist/jquery.sparkline.js"></script>
<!-- jQuery Sparklines -->
<!-- Flot -->
<script src="~/wwwroot/lib/gentelella/vendors/Flot/jquery.flot.js"></script>
<script src="~/wwwroot/lib/gentelella/vendors/Flot/jquery.flot.pie.js"></script>
<script src="~/wwwroot/lib/gentelella/vendors/Flot/jquery.flot.time.js"></script>
<script src="~/wwwroot/lib/gentelella/vendors/Flot/jquery.flot.stack.js"></script>
<script src="~/wwwroot/lib/gentelella/vendors/Flot/jquery.flot.resize.js"></script>
<!-- Flot plugins -->
<script src="~/wwwroot/lib/gentelella/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
<script src="~/wwwroot/lib/gentelella/vendors/flot-spline/js/jquery.flot.spline.js"></script>

<script src="~/wwwroot/lib/gentelella/vendors/flot.curvedlines/curvedLines.js"></script>

<script src="~/wwwroot/lib/gentelella/vendors/DateJS/build/date.js"></script>
<!-- DateJS -->
<!-- bootstrap-daterangepicker -->
<script src="~/wwwroot/lib/gentelella/vendors/moment/moment.js"></script>
<script src="~/wwwroot/lib/gentelella/vendors/bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js"></script>


<!-- Custom Theme Scripts -->
<script src="~/wwwroot/lib/gentelella/build/js/custom.js"></script>

</body>
</html>